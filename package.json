    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "pako": "^2.1.0",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "svgo": "^3.3.2",
    "svgo-loader": "^4.0.0",
    "uplot": "^1.6.31",
    "url-loader": "^4.1.1",
    "webpack": "^5.96.1",
